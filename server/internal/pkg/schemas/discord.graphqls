type Discord {
    id: String!
    joined: Boolean!
    VERIFIED_WITH: VERIFIED_WITH
    last_daily_at: Int
    name: String
    streak: Int
    level: Int
    xp: Int
}

extend type Query {
    discord(id: String!): Discord
}

extend type Mutation {
    createDiscord(id: String!, name: String!): Discord
    giveXp(id: String!, amount: Int!): Discord
}
